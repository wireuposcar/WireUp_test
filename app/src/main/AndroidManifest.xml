<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wireupfinland.wireup_test">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="WireUp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.Landingpage"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Login" />
        <activity android:name=".Activities.Main" />
        <activity android:name=".Activities.SetProject" />
        <activity android:name=".Activities.Project" />
        <activity android:name=".Activities.CreateAccount" />
        <activity android:name=".Activities.Chat" />
    </application>

</manifest>